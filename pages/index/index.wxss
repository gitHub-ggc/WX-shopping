.swiperView swiper {
  width: 750rpx;
  height: 320rpx;
}
.swiperView swiper image {
  width: 100%;
}
.cate {
  display: flex;
}
.cate .cate_item {
  flex: 1;
}
.cate .cate_item navigator {
  padding: 10rpx 40rpx;
}
.cate .cate_item navigator .cate_view {
  text-align: center;
}
.cate .cate_item navigator .cate_view image {
  width: 100%;
}
.cate .cate_item navigator .cate_text {
  text-align: center;
}
.cate .cate_item navigator .cate_text text {
  font-size: 24rpx;
}
.louchen .louchen_item {
  padding: 0 10rpx;
  overflow: hidden;
}
.louchen .louchen_item text {
  font-size: 24rpx;
  font-weight: 700;
  margin: 0 0 20rpx 20rpx;
  display: inline-block;
}
.louchen .louchen_item .louchen2 {
  overflow: hidden;
}
.louchen .louchen_item .louchen2 navigator {
  float: left;
  width: 33%;
  height: 400rpx;
}
.louchen .louchen_item .louchen2 navigator:nth-last-child(-n+2) {
  padding-top: 10rpx;
}
.louchen .louchen_item .louchen2 navigator:nth-last-child(-n+4) {
  height: 200rpx;
  padding-left: 10rpx;
}
.louchen .louchen_item .louchen2 navigator image {
  width: 100%;
  height: 100%;
}
